#!/usr/bin/env node
var lib = require('../lib')
  , self = lib.createReplicator(lib.config)

self.start(function (err) {
  if (err) {
    // TODO(schoon) - Real usage information.
    console.error('Error:', err.message || err)
    process.exit(1)
  }

  console.log('Replicating from %s to %s...', self.couchUrl, self.mongoUrl)
})
